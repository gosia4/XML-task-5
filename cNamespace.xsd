		<xs:restriction base="xs:string">
			<xs:pattern value="[a-z]*"/>
			<xs:minLength value="2"/>
			<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="instrumentsType">
		<xs:list itemType="c:instrumentListType"/>
	</xs:simpleType>
-->

	<xs:simpleType name="instrumentsType">
		<xs:restriction>
			<xs:simpleType>
				<xs:list>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1" />
							<xs:maxLength value="60" />
						</xs:restriction>
					</xs:simpleType>
				</xs:list>
			</xs:simpleType>
		</xs:restriction>
	</xs:simpleType>
	

		<xs:simpleType name="contentType">
			<xs:restriction base="xs:string">
				<xs:pattern value="[A-Z][a-z]*( [A-Z][a-z]*)*"/>
				<xs:minLength value="1" />
				<xs:maxLength value="30" />
			</xs:restriction>
		</xs:simpleType>


		<xs:simpleType name="pagesType">
			<xs:restriction base="xs:positiveInteger">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="500"/>
			</xs:restriction>
		</xs:simpleType>


		<xs:simpleType name="ISBNtype">
			<xs:restriction base="xs:string">
				<xs:pattern value="[0-9][0-9][0-9]-[0-9][0-9]-[0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]-[0-9]"/>
			</xs:restriction>
		</xs:simpleType>

	<xs:simpleType name="priceType">
		<xs:restriction base="xs:float"></xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="priceCurType">
		<xs:simpleContent>
			<xs:extension base="c:priceType">
				<xs:attribute name="cur">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="PLN"/>
							<xs:enumeration value="EUR"/>
							<xs:enumeration value="USD"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
			<!--<xs:pattern value="[0-9]*.[0-9][0-9]"/>-->
		</xs:simpleContent>
	</xs:complexType>


